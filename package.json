  },
  "homepage": "https://github.com/1380dev/ds#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
